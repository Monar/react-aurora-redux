{
  "name": "react-aurora-redux",
  "version": "0.0.2",
  "description": "Redux helpers for react-aurora",
  "main": "./lib/react-aurora-redux.js",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --fix .",
    "test": "jest ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monar/react-aurora-redux.git"
  },
  "keywords": [
    "react",
    "redux",
    "aurora",
    "helpers"
  ],
  "author": "Piotr Tomasz Monarski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Monar/react-aurora-redux/issues"
  },
  "homepage": "https://github.com/Monar/react-aurora-redux#readme",
  "peerDependencies": {
    "immutable": "^3"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-react": "6.24.1",
    "eslint": "4.8.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-class-property": "1.0.6",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.4.0",
    "immutable": "3.8.2",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "prettier": "1.7.4",
    "prop-types": "15.6.0",
    "rollup": "0.50.0",
    "rollup-plugin-babel": "3.0.2"
  },
  "jest": {
    "setupFiles": [
      "./testPolyfills.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost"
  },
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  }
}
