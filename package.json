{
  "name": "react-aurora-redux",
  "version": "0.0.3",
  "description": "Redux helpers for react-aurora",
  "main": "./lib/react-aurora-redux.js",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --fix .",
    "test": "jest ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monar/react-aurora-redux.git"
  },
  "keywords": [
    "react",
    "redux",
    "aurora",
    "helpers"
  ],
  "author": "Piotr Tomasz Monarski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Monar/react-aurora-redux/issues"
  },
  "homepage": "https://github.com/Monar/react-aurora-redux#readme",
  "dependencies": {
    "immutability-helper": "2.4.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "eslint": "4.8.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-class-property": "1.0.6",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-prettier": "2.3.1",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "prettier": "1.7.4",
    "rollup": "0.50.0",
    "rollup-plugin-babel": "3.0.2"
  },
  "jest": {
    "setupFiles": [
      "./testPolyfills.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost"
  },
  "babel": {
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  }
}
